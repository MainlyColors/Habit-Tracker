{
  "name": "habit-tracker",
  "version": "0.0.1",
  "description": "App for tracking habit completion",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon",
    "dev:server": "cross-env NODE_ENV=development npm run ts:exec server/server.ts",
    "check:prettier": "eslint-config-prettier ./server/server.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ts:exec": "ts-node -r tsconfig-paths/register --project ./tsconfig.json --swc"
  },
  "author": "Ryan Mahara",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "@swc/core": "^1.3.78",
    "@swc/helpers": "^0.5.1",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "nodemon": "^3.0.1",
    "prettier": "3.0.3",
    "regenerator-runtime": "^0.14.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  }
}
